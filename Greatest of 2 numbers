//BRUTE FORCE
#include <iostream>

using namespace std;

int main()
{
    int num1,num2;
    cin>>num1>>num2;
    if(num1>num2)
    {
        cout<<num1<<"is greater";
    }
    else
    cout<<num2<<"is greater";

    return 0;
}

//MAX FUNCTION
#include <iostream>
#include<math.h>
using namespace std;

int main()
{
    int num1,num2;
    cin>>num1>>num2;
    int ans=max(num1,num2);
    cout<<ans;
    return 0;
}

//TERNARY OPERATOR
#include <iostream>
#include<math.h>
using namespace std;

int main()
{
    int num1,num2;
    cin>>num1>>num2;
    int ans=(num1>num2)? num1:num2;
    cout<<ans<<"is greater";
    return 0;
}
